{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "labels": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "level1": {"type": "string", "minLength": 1},
          "level2": {"type": "string", "minLength": 1}
        },
        "required": ["level1", "level2"]
      }
    }
  },
  "required": ["labels"]
}
